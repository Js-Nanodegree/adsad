    "classnames": "^2.2.5",
    "express": "^4.16.2",
    "graphql": "^14.1.1",
    "graphql-anywhere": "^4.0.0",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.4",
    "http-proxy-middleware": "^0.19.1",
    "immutability-helper": "^3.0.0",
